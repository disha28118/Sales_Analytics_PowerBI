# Sales_Dashboard.pbix
# DAX Measures
Total Sales = SUM('Sales Orders'[Total Revenue])
Total Orders = DISTINCTCOUNT('Sales Orders'[OrderNumber])
Total Quantity = SUM('Sales Orders'[Order Quantity])
Total Cost = SUM('Sales Orders'[Total Unit Cost] * 'Sales Orders'[Order Quantity])
Profit = [Total Sales] - [Total Cost]
Profit Margin = DIVIDE([Profit], [Total Sales], 0)
Average Unit Price = AVERAGE('Sales Orders'[Unit Price])
Avg. Fulfillment Days = AVERAGE('Sales Orders'[Ship Date] - 'Sales Orders'[OrderDate])

